module tap_github

import "../../lib/meltano/plugin.pkl"

tap_github = new plugin.Extractor {
  name = "tap-github"
  variant = "meltanolabs"
  pip_url = "git+https://github.com/MeltanoLabs/tap-github.git@v1.3.0 python-json-logger"
  config = new Mapping {
    ["repositories"] = List(
      "edgarrmondragon/citric",
      "edgarrmondragon/tap-readthedocs"
    )
  }
  select = List(
    "repositories.*",
    "stargazers.*"
  )
}

